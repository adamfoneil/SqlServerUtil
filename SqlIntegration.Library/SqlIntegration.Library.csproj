<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Adam O'Neil</Authors>
    <Company>Adam O'Neil</Company>
    <Product>SqlIntegration.Library</Product>
    <Description>ETL library in progress for SQL Server, BulkInsert static class</Description>
    <Version>0.2.5-alpha</Version>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/adamosoftware/SqlIntegration</RepositoryUrl>
    <PackageProjectUrl>https://github.com/adamosoftware/SqlIntegration</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageReleaseNotes>added BulkInsert.OffsetExecuteAsync</PackageReleaseNotes>
    <AssemblyVersion>0.2.5.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Migrator.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AdoUtil.Library" Version="1.1.1" />
    <PackageReference Include="Dapper" Version="2.0.30" />
    <PackageReference Include="Dapper.CX.Base" Version="1.0.6" />
    <PackageReference Include="Dapper.CX.SqlServer" Version="1.0.4" />
    <PackageReference Include="Dapper.QX" Version="1.0.0" />
    <PackageReference Include="DataTables.Library" Version="2.0.1" />
    <PackageReference Include="System.Data.SqlClient" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
